{
  "name": "chaindown",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "npm run formatLocale && vue-cli-service serve",
    "build": "npm run createWebWorkerMap && vue-cli-service build",
    "lint": "vue-cli-service lint",
    "formatLocale": "node ./webpack-plugins/format-locale.js",
    "createWebWorkerMap": "node ./webpack-plugins/create-webWorker.js && node ./webpack-plugins/format-locale.js"
  },
  "dependencies": {
    "ali-oss": "^6.1.0",
    "axios": "^0.18.0",
    "babel-core": "^6.26.3",
    "bowser": "*",
    "crypto": "^1.0.1",
    "echarts": "^4.2.0-rc.2",
    "file-saver": "^2.0.1",
    "has": "^1.0.3",
    "pako": "^1.0.7",
    "rimraf": "^2.6.2",
    "safe-require": "^1.0.3",
    "v-click-outside-x": "^3.5.6",
    "vue": "^2.5.17",
    "vue-i18n": "^8.5.0",
    "vue-router": "^3.0.1",
    "vue-script2": "*",
    "vuescroll": "^4.9.0-beta.18",
    "vuex": "^3.0.1",
    "xlsx": "^0.14.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.2.0",
    "@vue/cli-plugin-eslint": "^3.2.1",
    "@vue/cli-service": "^3.2.0",
    "@vue/eslint-config-airbnb": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-vue": "^5.0.0-0",
    "lint-staged": "^7.2.2",
    "script-loader": "^0.7.2",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "vue-template-compiler": "^2.5.17"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
